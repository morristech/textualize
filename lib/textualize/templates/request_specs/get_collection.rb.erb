require 'airborne'

describe 'GET <%= url %>' do
  let!(:<%= name %>) { create_list(:<%= name %>, 3) }

  <%= secured_by %>.each do |role|
    it "returns expected json for #{role} role" do
      add_header_token role
      get "<%= url %>"
      expect_status <%= response_code %>
      expect_json_keys <%= body.data.sample.keys %>
    end
  end
end

