require 'airborne'

describe 'GET <%= url %>' do
  let!(:<%= name %>) { create(:<%= name %>) }

  before do
    add_header_tokens(<%= secured_by %>)
  end

  it 'returns expected json' do
    get "<%= url %>"
    expect_status <%= response_code %>
    expect_json_keys <%= body.data.keys %>
  end
end

